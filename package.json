{
  "name": "front-end-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "description": "웹 사이트를 빠르게 구축하기 위한 front-end template입니다.",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaem1n207/front-end-template.git"
  },
  "keywords": [
    "boilerplate",
    "javascript"
  ],
  "bugs": "https://github.com/jaem1n207/front-end-template/issues",
  "author": "@jaem1n207 (roy.jm.lee@gmail.com)",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development webpack serve --open",
    "build": "NODE_ENV=production webpack",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(js)\"",
    "lint-staged": "lint-staged",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(js)\" --write",
    "prepare": "husky install",
    "commit": "cz",
    "remove:example": "rm -R example"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.17.10",
    "@babel/plugin-transform-runtime": "7.17.10",
    "@babel/preset-env": "7.17.10",
    "@commitlint/cli": "16.3.0",
    "@commitlint/config-conventional": "16.2.4",
    "babel-loader": "8.2.5",
    "clean-webpack-plugin": "4.0.0",
    "commitizen": "4.2.4",
    "css-loader": "6.7.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.15.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "file-loader": "6.2.0",
    "html-loader": "3.1.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.1",
    "lint-staged": "12.4.1",
    "mini-css-extract-plugin": "2.6.0",
    "node-sass": "7.0.1",
    "prettier": "2.6.2",
    "sass-loader": "12.6.0",
    "style-loader": "3.3.1",
    "terser-webpack-plugin": "5.3.1",
    "url-loader": "4.1.1",
    "webpack": "5.72.1",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.9.0"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "7.17.9",
    "axios": "0.27.2",
    "core-js": "3.22.5",
    "dotenv": "16.0.1"
  },
  "engines": {
    "node": ">=13"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
